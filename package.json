  "bugs": {
    "url": "https://github.com/Bryukh/graphs/issues"
  },
  "homepage": "https://github.com/Bryukh/graphs#readme",
  "devDependencies": {
    "chai": "^3.3.0",
    "commitizen": "^1.0.4",
    "cz-conventional-changelog": "^1.1.2",
    "ghooks": "^0.3.2",
    "istanbul": "^0.3.21",
    "mocha": "^2.3.3"
  },
  "czConfig": {
    "path": "node_modules/cz-conventional-changelog"
